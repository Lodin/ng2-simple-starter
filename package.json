{
  "name": "ng2-simple-starter",
  "version": "0.0.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "start": "ng serve",
    "postinstall": "typings install",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "npm run test:dev",
    "test:dev": "cross-env TEST_ENV=development ng test",
    "test:prod": "cross-env TEST_ENV=production ng test",
    "prebuild": "ng build",
    "inline": "gulp inline --gulpfile config/gulp.config.js --cwd .",
    "webpack:dev": "cross-env BUILD_ENV=development webpack --config=config/webpack.config.js",
    "webpack:prod": "cross-env BUILD_ENV=production webpack --config=config/webpack.config.js",
    "build:dev": "npm run prebuild && npm run inline && npm run webpack:dev",
    "build:prod": "npm run prebuild && npm run inline && npm run webpack:prod",
    "pree2e": "webdriver-manager update",
    "e2e": "protractor"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "2.0.0-rc.2",
    "@angular/compiler": "2.0.0-rc.2",
    "@angular/core": "2.0.0-rc.2",
    "@angular/http": "2.0.0-rc.2",
    "@angular/platform-browser": "2.0.0-rc.2",
    "@angular/platform-browser-dynamic": "2.0.0-rc.2",
    "@angular/router": "3.0.0-alpha.6",
    "babel-polyfill": "^6.9.1",
    "babel-runtime": "^6.9.2",
    "core-js": "^2.4.0",
    "es6-shim": "0.35.1",
    "reflect-metadata": "0.1.3",
    "rxjs": "5.0.0-beta.6",
    "systemjs": "0.19.26",
    "ts-helpers": "^1.1.1",
    "zone.js": "0.6.12"
  },
  "devDependencies": {
    "angular-cli": "1.0.0-beta.6",
    "autoprefixer": "^6.3.6",
    "babel-plugin-external-helpers-2": "^6.3.13",
    "babel-plugin-transform-export-extensions": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "broccoli-babel-plugin": "^0.3.0",
    "broccoli-funnel": "^1.0.1",
    "broccoli-merge-trees": "^1.1.1",
    "broccoli-postcss": "^3.0.0",
    "broccoli-pug-render": "^1.0.4",
    "codelyzer": "0.0.20",
    "cross-env": "^1.0.8",
    "ember-cli-inject-live-reload": "1.4.0",
    "gulp": "^3.9.1",
    "gulp-flatmap": "^1.0.0",
    "gulp-inline-ng2-template": "^2.0.4",
    "html-webpack-plugin": "^2.21.0",
    "istanbul": "^0.4.3",
    "jasmine-core": "^2.4.1",
    "jasmine-spec-reporter": "^2.5.0",
    "karma": "^0.13.22",
    "karma-chrome-launcher": "^1.0.1",
    "karma-coverage": "^1.0.0",
    "karma-firefox-launcher": "^1.0.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.0",
    "node-sass": "^3.7.0",
    "phantomjs-prebuilt": "^2.1.7",
    "postcss-inline-svg": "^2.0.1",
    "protractor": "^3.3.0",
    "script-loader": "^0.7.0",
    "ts-node": "^0.9.1",
    "tslint": "^3.11.0",
    "typescript": "^1.9.0-dev.20160615-1.0",
